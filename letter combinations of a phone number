class Solution(object):
    def letterCombinations(self, digits):
        """
        :type digits: str
        :rtype: List[str]
        """
        if not digits:
            return []
        idic={2:'abc',3:'def',4:'ghi',5:'jkl',6:'mno',7:'pqrs',8:'tuv',9:'wxyz'}
        res=['']
        for i in map(int,list(digits)):
            cur=[]
            for j in res:
                for k in idic[i]:
                    cur.append(j+k)
            res=cur
        return res
